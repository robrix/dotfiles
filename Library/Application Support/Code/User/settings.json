{
    "terminal.integrated.macOptionIsMeta": true,
    "explorer.sortOrder": "mixed",
    "editor.fontFamily": "FiraCode-Retina",
    "editor.tabSize": 2,
    "editor.scrollBeyondLastLine": false,
    "editor.wordWrap": "on",
    "languageServerHaskell.trace.server": "messages",
    "workbench.startupEditor": "newUntitledFile",
    "editor.minimap.enabled": false,
    "editor.fastScrollSensitivity": 1,
    "editor.cursorBlinking": "solid",
    "editor.cursorWidth": 2,
    "editor.fontLigatures": "'onum'",
    "workbench.commandPalette.preserveInput": true,
    "workbench.iconTheme": "eq-material-theme-icons-darker",
    "workbench.tips.enabled": false,
    "workbench.tree.indent": 16,
    "breadcrumbs.enabled": false,
    "workbench.editor.closeEmptyGroups": false,
    "workbench.editor.openPositioning": "last",
    "workbench.editor.revealIfOpen": true,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "window.restoreFullscreen": true,
    "window.restoreWindows": "all",
    "languageServerHaskell.enableHIE": false,
    "window.zoomLevel": 0,
    "workbench.activityBar.visible": false,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "terminal.integrated.rendererType": "canvas",
    "editor.roundedSelection": false,
    "editor.selectionHighlight": false,
    "editor.emptySelectionClipboard": false,
    "files.exclude": {
        "**/.licenses": true,
        "**/dist": true,
        "**/dist-newstyle": true,
        "**/dist-repl": true,
        "semantic/test/fixtures/**": true
    },
    "editor.smoothScrolling": true,
    "editor.occurrencesHighlight": false,
    "editor.renderLineHighlight": "all",
    "workbench.editor.highlightModifiedTabs": true,
    "search.useGlobalIgnoreFiles": true,
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": "comment",
                "settings": {
                    "foreground": "#a0a0a0",
                    "fontStyle": ""
                }
            },
            {
                "scope": "constant.other",
                "settings": {
                    "foreground": "#ff6e6e",
                }
            },
            {
                "scope": "keyword.control",
                "settings": {
                    "fontStyle": ""
                }
            },
            {
                "scope": "meta.import.qualifier",
                "settings": {
                    "foreground": "#ffcb6bff"
                }
            }
        ]
    },
    "workbench.colorCustomizations": {
        "[GitHub Dark]": {
            "editor.background": "#14161a",
            "terminal.background": "#14161a",
        },
        "activityBarBadge.background": "#64FFDA",
        "list.activeSelectionForeground": "#64FFDA",
        "list.inactiveSelectionForeground": "#64FFDA",
        "list.highlightForeground": "#64FFDA",
        "scrollbarSlider.activeBackground": "#64FFDA50",
        "editorSuggestWidget.highlightForeground": "#64FFDA",
        "textLink.foreground": "#64FFDA",
        "progressBar.background": "#64FFDA",
        "pickerGroup.foreground": "#64FFDA",
        "tab.activeBorder": "#64FFDA",
        "notificationLink.foreground": "#64FFDA",
        "editorWidget.resizeBorder": "#64FFDA",
        "editorWidget.border": "#64FFDA",
        "settings.modifiedItemIndicator": "#64FFDA",
        "settings.headerForeground": "#64FFDA",
        "panelTitle.activeBorder": "#64FFDA",
        "breadcrumb.activeSelectionForeground": "#64FFDA",
        "menu.selectionForeground": "#64FFDA",
        "menubar.selectionForeground": "#64FFDA",
        "editor.findMatchBorder": "#64FFDA",
        "selection.background": "#64FFDA40",
        "terminal.ansiBlack": "#000000",
        "terminal.ansiRed": "#990000",
        "terminal.ansiGreen": "#00A600",
        "terminal.ansiYellow": "#999900",
        "terminal.ansiBlue": "#2d2dff",
        "terminal.ansiMagenta": "#B200B2",
        "terminal.ansiCyan": "#00A6B2",
        "terminal.ansiWhite": "#e5e5e5",
        "terminal.ansiBrightBlack": "#666666",
        "terminal.ansiBrightRed": "#E50000",
        "terminal.ansiBrightGreen": "#00D900",
        "terminal.ansiBrightYellow": "#E5E500",
        "terminal.ansiBrightBlue": "#5252ff",
        "terminal.ansiBrightMagenta": "#E500E5",
        "terminal.ansiBrightCyan": "#00E5E5",
        "terminal.ansiBrightWhite": "#ffffff"
    },
    "materialTheme.accent": "Teal",
    "files.insertFinalNewline": true,
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "terminal.integrated.scrollback": 10000,
    "workbench.editor.focusRecentEditorAfterClose": false,
    "dash.languageIdToDocsetMap.haskell": "hackage",
    "files.hotExit": "onExitAndWindowClose",
    "workbench.editor.showTabs": true,
    "editor.renderControlCharacters": true,
    "editor.find.addExtraSpaceOnTop": false,
    "editor.find.globalFindClipboard": true,
    "editor.find.seedSearchStringFromSelection": "never",
    "haskell.hlint.logLevel": "warn",
    "haskell.hlint.run": "onSave",
    "extensions.ignoreRecommendations": true,
    "git.confirmSync": false,
    "haskell.formattingProvider": "stylish-haskell",
    "[haskell]": {
        "editor.defaultFormatter": "haskell.haskell"
    },
    "terminal.integrated.wordSeparators": " ()[]{}',\"`â”€-.=/:;<>?\\+*&^%$#@!~",
    "terminal.integrated.commandsToSkipShell": [
        "workbench.action.quickSwitchWindow"
    ],
    "outline.showModules": false,
    "terminal.integrated.experimentalUseTitleEvent": true,
    "workbench.editor.pinnedTabSizing": "compact",
    "editor.formatOnSave": true,
    "editor.suggest.insertMode": "replace",
    "editor.suggestSelection": "recentlyUsedByPrefix",
    "errorLens.scrollbarHackEnabled": true,
    "security.workspace.trust.untrustedFiles": "open",
    "editor.linkedEditing": true,
    "html.format.wrapLineLength": 0,
    "html.format.enable": false,
    "emmet.showSuggestionsAsSnippets": true,
    "extensions.supportUntrustedWorkspaces": {
        "haskell.haskell": {
            "supported": true,
            "version": "1.6.0"
        },
    },
    "extensions.closeExtensionDetailsOnViewChange": true,
    "editor.suggest.showStatusBar": true,
    "editor.accessibilitySupport": "off",
    "terminal.integrated.profiles.osx": {
        "ghc": {
            "path": "/usr/local/bin/fish",
            "args": []
        }
    },
    "terminal.integrated.defaultProfile.osx": "ghc",
    "window.autoDetectColorScheme": true,
    "editor.cursorSurroundingLines": 1,
    "terminal.integrated.cursorWidth": 2,
    "problems.sortOrder": "position",
    "problems.showCurrentInStatus": true,
    "errorLens.delay": 500,
    "terminal.integrated.tabs.enabled": false
}
